{
  "rewrites": [
    { "source": "/", "destination": "blog/public/index.html" },
    { "source": "/demo/:match*/", "destination": ":match*/dist/index.html" },
    { "source": "/demo/:match*/:match1*", "destination": ":match*/dist/:match1*" },
    { "source": "/demo/react-redux", "destination": "react-redux/products/dist/index.html" },
    { "source": "/:match*/", "destination": "blog/public/:match*/index.html" },
    { "source": "/:match*", "destination": "blog/public/:match*" }
  ]
}